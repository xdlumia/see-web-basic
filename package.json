{
  "name": "see-web-basic",
  "description": "see-web-basic project",
  "version": "1.4.29",
  "author": "yanchao",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "see-copy-file": "bin/copy-file.js",
    "see-publish": "bin/see-publish.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "npm run build:js && npm run build:copy",
    "build:js": "cross-env BABEL_ENV=utils babel src --out-dir dist",
    "build:bin": "cross-env BABEL_ENV=node babel bin --out-dir dist/bin",
    "build:copy": "node bin/copy-file.js  \"src/**/*.!(js)\" dist",
    "clean": "rimraf node_modules",
    "pub": "npm run build && node bin/see-publish.js",
    "pub:beta": "npm run build && node bin/see-publish.js beta"
  },
  "dependencies": {
    "@riophae/vue-treeselect": "^0.0.38",
    "readline-sync": "1.4.9"
  },
  "peerDependencies": {
    "vue": "^2.6.10",
    "axios": "^0.19.0",
    "element-ui": "^2.4.7",
    "moment": "^2.22.2",
    "vuedraggable": "^2.19.0"
  },
  "browserslist": [
    "> 1%",
    "not ie <= 8"
  ],
  "devDependencies": {
    "moment": "^2.22.2",
    "babel-cli": "^6.22.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "aglob": "^3.0.1",
    "rimraf": "^2.5.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^7.0.0",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "publishConfig": {
    "set": "npm config set registry http://39.96.212.175:8889/repository/npm-internal/",
